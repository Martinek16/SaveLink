{
  "name": "SaveLink",
  "version": "2.0",
  "description": "The extension allows you to save URLs",
  "permissions": [
      "tabs"
  ],
  "browser_action": {
      "default_popup": "index.html"
  },
  "action": {
      "default_popup": "index.html",
      "default_icon": {
          "16": "img/logo16.png",
          "48": "img/logo48.png",
          "128": "img/logo128.png"
      }
  },
  "icons": {
      "16": "img/logo16.png",
      "48": "img/logo48.png",
      "128": "img/logo128.png"
  },
  "manifest_version": 2
}